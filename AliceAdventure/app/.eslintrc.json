{
    "extends": "airbnb-base",
    "rules": {
      "no-unused-vars":"warn",
      "linebreak-style":"off",
      "no-alert":"warn",
      "global-require":"warn",
      "no-restricted-globals": "warn",
      "max-len": "warn"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "Vue": true
    }
}